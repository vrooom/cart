{% extends 'layout.html.twig' %}

{% block content %}

    <h1>{{ form_name|default('Form')|trans }}</h1>

    <form action="" method="post" novalidate {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <div class="actions">
            <input type="submit" value="{{ 'Send'|trans }}" class="btn primary">
        </div>
    </form>

{% endblock %}
